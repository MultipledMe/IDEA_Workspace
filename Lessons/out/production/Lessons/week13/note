@Accessing Directory
$$
public class Access_directory{
static void Container(File f){
    if(f.isDirectory()){
        File []result;
        result = f.listFiles();
        if(result != null){
               for(int i=0;i<result.length;i++){
                    return Container(result[i])
                    }
               }
        }
    }

